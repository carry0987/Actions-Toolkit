{
  "name": "@carry0987/actions-toolkit",
  "version": "1.0.0",
  "description": "Toolkit for Github Actions",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build:js": "rollup --config rollup.config.ts --configPlugin typescript --environment BUILD:js",
    "build:dts": "rollup --config rollup.config.ts --configPlugin typescript --environment BUILD:dts",
    "build": "npm run build:js && npm run build:dts",
    "lint": "prettier --write \"src/**/*.{ts,tsx,scss}\" \"test/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carry0987/actions-toolkit.git"
  },
  "keywords": [
    "github",
    "actions"
  ],
  "author": "carry0987",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carry0987/actions-toolkit/issues"
  },
  "homepage": "https://github.com/carry0987/actions-toolkit#readme",
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@actions/cache": "^4.1.0",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.2",
    "csv-parse": "^6.1.0",
    "jwt-decode": "^4.0.0",
    "tmp": "^0.2.5"
  },
  "devDependencies": {
    "@octokit/core": "^7.0.4",
    "@octokit/openapi-types": "^26.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^16.1.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.5.2",
    "@types/tmp": "^0.2.6",
    "@vitest/coverage-v8": "3.2.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "sass": "^1.93.2",
    "tslib": "^2.8.1",
    "vitest": "^3.2.4"
  }
}
